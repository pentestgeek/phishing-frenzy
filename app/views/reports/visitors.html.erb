<% @page_title = "Phishing Visitors" %>

<h2>Visitors: <%= @campaign.name %></h2>

<% counter = 1 %>
<% @visitors.each do |visitor| %>
	<table>
	 <tr class="<%= cycle('odd', 'even') %>">
	 	<td width="350">
			<p><%= simple_format(visitor) %></p>
			<% counter += 1 %>
		</td>
	 </tr>
	</table>
<% end %>